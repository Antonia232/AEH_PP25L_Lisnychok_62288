import java.util.Scanner;

public class main {
    public static void main(String[] args) {
   
        Scanner scanner = new Scanner(System.in);

     
        System.out.print("Podaj swój wiek w latach: ");
        int ageYears = scanner.nextInt();

       
        int ageSeconds = ageYears * 365 * 24 * 60 * 60;

       
        System.out.println("Twój wiek w sekundach: " + ageSeconds);

        
        scanner.close();
    }
}
